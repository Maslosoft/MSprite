{
	"name": "maslosoft/sprite",
	"description": "Icons sprite generator with signals support",
	"keywords" : ["sprite", "sprites", "icon", "icons", "yii"],
	"license": [
		"AGPL",
		"Commercial"
	],
	"authors": [
		{
			"name": "Peter Maselkowski",
			"email": "pmaselkowski@gmail.com",
			"homepage": "https://maslosoft.com",
			"role": "Developer"
		}
	],
	"bin":[
		"sprite"
	],
	"autoload": {
        "psr-4": {
			  "Maslosoft\\Sprite\\" : "src",
        	  "Maslosoft\\SpriteTest\\Extensions\\": "tests/extensions"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Maslosoft\\SpriteTest\\Models\\": "tests/models"
		}
	},
	"require": {
		"maslosoft/miniview": "*",
		"latte/latte": ">=1.0 <3.0",
		"maslosoft/embedi": "*",
		"maslosoft/signals": "*",
		"symfony/console": ">=2.7 <5.0",
		"maslosoft/sitcom": "*",
		"symfony/finder": ">=2.7 <5.0",
		"maslosoft/sigurd": "*",
		"psr/log": "^1",
		"maslosoft/cli-shared": "*"
	},
	"require-dev": {
		"codeception/codeception": "^2",
		"maslosoft/hedron": "*"
	}
}
