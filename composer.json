{
	"name": "maslosoft/sprite",
	"type": "yii-extension",
	"description": "Sprite generator for Yii",
	"keywords" : ["sprite", "sprites", "icon", "icons", "yii"],
	"license": "GNU LESSER GENERAL PUBLIC LICENSE",
	"authors": [
		{
			"name": "Peter Maselkowski",
			"email": "pmaselkowski@gmail.com",
			"homepage": "http://maslosoft.com",
			"role": "Developer"
		}
	],
	"bin":[
		"sprite"
	],
	"autoload": {
        "psr-4": {
			  "Maslosoft\\Sprite\\" : "src"
        }
    },
	"require": {
		"maslosoft/signals": "*",
		"symfony/console": "^2",
		"maslosoft/sitcom": "*",
		"symfony/finder": "^2.7",
		"maslosoft/sigurd": "*"
	},
	"require-dev": {
		"maslosoft/sprite-test": "@dev"
	}
}
